<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="skycable_inventory_adjustment_sequence" model="ir.sequence">
            <field name="name">Skycable Increment Sequence</field>
            <field name="code">skycable.inventory.adjustment.sequence</field>
            <field name="prefix">EKS-%(month)s-%(day)s-%(year)s-</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
        </record>

        <record id="team_sequence" model="ir.sequence" >
            <field name="name">team.sequence</field>
            <field name="code">team_sequence</field>
            <field name="prefix">EKS_</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>